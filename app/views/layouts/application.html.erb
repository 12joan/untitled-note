<!DOCTYPE html>
<html>
  <head>
    <title>Untitled Note App</title>
    <meta name="viewport" content="width=device-width,initial-scale=1,viewport-fit=cover">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= vite_client_tag %>
    <%= vite_react_refresh_tag %>

    <%= vite_stylesheet_tag 'application.scss' %>

    <%= yield :head %>
  </head>

  <body class="min-h-screen text-ui bg-page-bg-light ring-offset-page-bg-light dark:bg-page-bg-dark dark:ring-offset-page-bg-dark">
    <%= yield %>
  </body>
</html>
